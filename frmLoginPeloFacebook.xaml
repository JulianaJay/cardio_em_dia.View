<Window x:Class="cardio_em_dia.frmLoginPeloFacebook"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cardio_em_dia"
        mc:Ignorable="d"
        Title="frmLoginPeloFacebook" Height="850" Width="440"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize">
    <Grid>
        <Image HorizontalAlignment="Center" Height="685" VerticalAlignment="Top" Width="430" Source="/LogFace.PNG" Margin="0,51,0,0">
            <Image.OpacityMask>
                <ImageBrush ImageSource="/LogFace.PNG"/>
            </Image.OpacityMask>
        </Image>
        <Image x:Name="imgAgoraNao" HorizontalAlignment="Center" Height="36" Margin="0,433,0,0" VerticalAlignment="Top" Width="104" Source="/Agora não.PNG" MouseLeftButtonDown="VoltarAoLogin">
            <Image.OpacityMask>
                <ImageBrush ImageSource="/Agora não.PNG"/>
            </Image.OpacityMask>
        </Image>
    </Grid>
</Window>
