<Window x:Class="cardio_em_dia.FrmSubDiarioDoDia"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cardio_em_dia"
        mc:Ignorable="d"
        Title="FrmSubDiarioDoDia" Height="850" Width="440"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid>
            <Image Source="/Sub-Tela  Diário - CardioEmDia.jpg" Stretch="UniformToFill"/>
            <Button x:Name="btnVoltarParATelaDiario" Content="" HorizontalAlignment="Left" Margin="14,19,0,0" VerticalAlignment="Top" Height="30" Width="45" Click="VoltarParaODiario">
                <Button.Background>
                    <ImageBrush/>
                </Button.Background>
                <Button.BorderBrush>
                    <ImageBrush/>
                </Button.BorderBrush>
            </Button>
        </Grid>

    </Grid>
</Window>
